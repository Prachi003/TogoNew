    android:id="@+id/swipe_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/_5sdp"
    app:clickToClose="true">

    <LinearLayout
        android:id="@+id/delete_post"
        android:layout_width="@dimen/_66sdp"
        android:layout_height="wrap_content"
        android:background="@color/colorWhite"
        android:gravity="center"
        android:visibility="gone"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/_2sdp"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_gravity="center"
                android:layout_margin="@dimen/_5sdp"
                app:srcCompat="@drawable/ic_new_delete_post" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="@font/montserrat_medium"
                android:gravity="center"
                android:text="@string/delete"
                android:textColor="@color/new_cancel_color"
                android:textSize="@dimen/_12sdp" />

        </LinearLayout>

    </LinearLayout>



    <LinearLayout
        android:id="@+id/main_click"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:orientation="horizontal"
            android:background="@drawable/new_change_password_items_bg">

            <LinearLayout
                android:layout_width="100dp"
                android:gravity="center"
                android:layout_gravity="center"
                android:background="@drawable/new_change_color_blue"
                android:layout_height="match_parent"
                android:orientation="vertical">



                <TextView
                    android:id="@+id/txtPrice"
                    android:layout_width="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/rubik_medium"
                    android:maxLines="1"
                    android:text="$ 125.0"
                    android:textColor="@color/colorWhite"
                    android:textSize="@dimen/textNormalSize" />

                <TextView
                    android:id="@+id/title"
                    android:layout_width="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/rubik_regular"
                    android:maxLines="2"
                    android:text="@string/total_delivery_price_n"
                    android:textColor="@color/colorWhite"
                    android:textSize="11sp" />


            </LinearLayout>


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="wrap_content"
                    android:text="Laptop orders"
                    android:textSize="16sp"
                    android:id="@+id/txtTitleNew"
                    android:textColor="@color/colorPrimary"
                    android:fontFamily="@font/rubik_medium"
                    android:layout_margin="@dimen/_7sdp"
                    android:layout_height="wrap_content" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:layout_below="@+id/txtTitleNew"

                    android:id="@+id/llOrders"
                    android:layout_marginBottom="@dimen/_10sdp"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:layout_alignParentBottom="true"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/_5sdp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:text="05"
                        android:textSize="14sp"
                        android:id="@+id/txtLaptopOrderQuantity"
                        android:textColor="@color/colorBlack"
                        android:fontFamily="@font/rubik_medium"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:text="Delivery items"
                        android:textSize="12sp"
                        android:id="@+id/txtDelivery"
                        android:layout_below="@+id/txtLaptopOrderQuantity"
                        android:layout_marginBottom="@dimen/_5sdp"
                        android:textColor="@color/darke_gray"
                        android:fontFamily="@font/rubik_regular"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:text="1 day ago"
                        android:id="@+id/txtTime"
                        android:textSize="12sp"
                        android:layout_marginRight="4sp"
                        android:layout_below="@+id/txtLaptopOrder"

                        android:layout_centerVertical="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginBottom="@dimen/_10sdp"
                        android:fontFamily="@font/rubik_regular"

                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true" />


                </RelativeLayout>


            </RelativeLayout>

