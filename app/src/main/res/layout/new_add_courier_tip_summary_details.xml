                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:fontFamily="@font/rubik_regular"
                    android:paddingRight="@dimen/_10sdp"
                    android:singleLine="true"
                    android:text="@string/item_quantity_heading"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/textSmallSize" />

                <TextView
                    android:id="@+id/tv_item_quantity"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:ellipsize="end"
                    android:fontFamily="@font/rubik_regular"
                    android:maxLines="1"
                    android:paddingRight="@dimen/_10sdp"
                    android:singleLine="true"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/textSmallSize" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5sdp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start"
                    android:fontFamily="@font/rubik_regular"
                    android:paddingRight="@dimen/_10sdp"
                    android:text="@string/item_description_heading"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/textSmallSize" />

                <TextView
                    android:id="@+id/tv_item_description"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start"
                    android:layout_marginTop="@dimen/_3sdp"
                    android:layout_marginRight="@dimen/_10sdp"
                    android:fontFamily="@font/rubik_regular"
                    android:paddingRight="@dimen/_10sdp"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/textSmallSize" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <View
                android:layout_width="match_parent"
                android:layout_height="0.3dp"
                android:layout_gravity="end"
                android:layout_marginTop="@dimen/_10sdp"
                android:background="@color/new_add_item_line_color" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_10sdp">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_gravity="center"
                    android:fontFamily="@font/rubik_medium"
                    android:gravity="start"
                    android:text="@string/delivery_amount"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/textNormalSize" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:fontFamily="@font/rubik_medium"
                        android:gravity="start"
                        android:text="@string/dollar"
                        android:textColor="@color/colorBlack"
                        android:textSize="@dimen/textNormalSize" />

                    <TextView
                        android:id="@+id/tv_delivery_amount"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:fontFamily="@font/rubik_medium"
                        android:gravity="start"
                        android:text="@string/zero_amount"
                        android:textColor="@color/colorBlack"
                        android:textSize="@dimen/textNormalSize" />
                </LinearLayout>
            </RelativeLayout>

        </LinearLayout>

