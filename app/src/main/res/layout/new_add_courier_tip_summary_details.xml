<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10sdp"
        android:gravity="center"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="@dimen/_15sdp"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/_3sdp"
            android:gravity="top"
            android:orientation="vertical">

            <View
                android:layout_width="@dimen/_12sdp"
                android:layout_height="@dimen/_12sdp"
                android:layout_gravity="center"
                android:background="@drawable/new_app_color_stroke_bg" />

            <View
                android:layout_width="@dimen/_2sdp"
                android:layout_height="@dimen/_45sdp"
                android:layout_gravity="center"
                android:background="@color/new_app_color" />

            <View
                android:layout_width="@dimen/_12sdp"
                android:layout_height="@dimen/_12sdp"
                android:layout_gravity="center"
                android:background="@drawable/new_continue_btn_bg" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_sel_pickup_address"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:ellipsize="end"
                    android:fontFamily="@font/rubik_regular"
                    android:gravity="start"
                    android:maxLines="1"
                    android:paddingLeft="@dimen/_5sdp"
                    android:paddingRight="@dimen/_10sdp"
                    android:singleLine="true"
                    android:text="@string/pickup_address"
                    android:textColor="@color/new_sign_in_text_color"
                    android:textSize="@dimen/textNormalSize" />

                <TextView
                    android:id="@+id/tv_sel_pickup_date_time"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:ellipsize="end"
                    android:fontFamily="@font/rubik_light"
                    android:gravity="start"
                    android:maxLines="1"
                    android:paddingLeft="@dimen/_5sdp"
                    android:paddingRight="@dimen/_10sdp"
                    android:singleLine="true"
                    android:textColor="@color/new_pickup_delivery_date_tym_color"
                    android:textSize="@dimen/textSmallSize" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.3dp"
                    android:layout_gravity="end"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@color/new_add_item_line_color" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_sel_drop_off_address"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:ellipsize="end"
                    android:fontFamily="@font/rubik_regular"
                    android:gravity="start"
                    android:maxLines="1"
                    android:paddingLeft="@dimen/_5sdp"
                    android:paddingRight="@dimen/_10sdp"
                    android:singleLine="true"
                    android:text="@string/drop_off_address"
                    android:textColor="@color/new_sign_in_text_color"
                    android:textSize="@dimen/textNormalSize" />

                <TextView
                    android:id="@+id/tv_sel_drop_off_date_time"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:ellipsize="end"
                    android:fontFamily="@font/rubik_light"
                    android:gravity="start"
                    android:maxLines="1"
                    android:paddingLeft="@dimen/_5sdp"
                    android:paddingRight="@dimen/_10sdp"
                    android:singleLine="true"
                    android:textColor="@color/new_pickup_delivery_date_tym_color"
                    android:textSize="@dimen/textSmallSize" />

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10sdp"
        android:orientation="vertical">

        <View
            android:layout_width="match_parent"
            android:layout_height="0.3dp"
            android:layout_gravity="end"
            android:background="@color/new_add_item_line_color" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/_10sdp"
            android:fontFamily="@font/rubik_regular"
            android:gravity="start"
            android:text="@string/person_at_pickup_location"
            android:textColor="@color/new_sign_in_text_color"
            android:textSize="@dimen/textNormalSize" />

        <LinearLayout
            android:id="@+id/ly_someone_pickup_person"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_pickup_person_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/_5sdp"
                android:ellipsize="end"
                android:fontFamily="@font/rubik_regular"
                android:gravity="start"
                android:maxLines="1"
                android:paddingRight="@dimen/_10sdp"
                android:singleLine="true"
                android:text="@string/enter_first_name"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textSmallSize" />

            <LinearLayout
                android:id="@+id/ly_pick_contact"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_20sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:fontFamily="@font/rubik_regular"
                    android:gravity="center"
                    android:text="+1"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/textSmallSize" />

                <View
                    android:layout_width="1dp"
                    android:layout_height="@dimen/_10sdp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:background="@color/new_add_item_line_color" />

                <TextView
                    android:id="@+id/tv_pickup_person_contact"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:ellipsize="end"
                    android:fontFamily="@font/rubik_regular"
                    android:gravity="start"
                    android:maxLines="1"
                    android:paddingRight="@dimen/_10sdp"
                    android:singleLine="true"
                    android:text="@string/enter_contact_number"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/textSmallSize" />

            </LinearLayout>

            <TextView
                android:id="@+id/tv_pick_contact_na"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/_5sdp"
                android:ellipsize="end"
                android:fontFamily="@font/rubik_regular"
                android:gravity="start"
                android:maxLines="1"
                android:paddingRight="@dimen/_10sdp"
                android:singleLine="true"
                android:text="@string/na_txt"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textSmallSize"
                 android:visibility="gone"/>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.3dp"
            android:layout_gravity="end"
            android:layout_marginTop="@dimen/_10sdp"
            android:background="@color/new_add_item_line_color" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/_10sdp"
            android:fontFamily="@font/rubik_regular"
            android:gravity="start"
            android:text="@string/person_at_delivery_location"
            android:textColor="@color/new_sign_in_text_color"
            android:textSize="@dimen/textNormalSize" />

        <LinearLayout
            android:id="@+id/ly_someone_delivery_person"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_delivery_person_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/_5sdp"
                android:ellipsize="end"
                android:fontFamily="@font/rubik_regular"
                android:gravity="start"
                android:maxLines="1"
                android:paddingRight="@dimen/_10sdp"
                android:singleLine="true"
                android:text="@string/enter_first_name"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textSmallSize" />

            <LinearLayout
                android:id="@+id/ly_deliver_contact"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_20sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:fontFamily="@font/rubik_regular"
                    android:gravity="center"
                    android:text="+1"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/textSmallSize" />

                <View
                    android:layout_width="1dp"
                    android:layout_height="@dimen/_10sdp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:background="@color/new_add_item_line_color" />

                <TextView
                    android:id="@+id/tv_delivery_person_contact"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:ellipsize="end"
                    android:fontFamily="@font/rubik_regular"
                    android:gravity="start"
                    android:maxLines="1"
                    android:paddingRight="@dimen/_10sdp"
                    android:singleLine="true"
                    android:text="@string/enter_contact_number"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/textSmallSize" />

            </LinearLayout>

            <TextView
                android:id="@+id/tv_deliver_contact_na"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/_5sdp"
                android:ellipsize="end"
                android:fontFamily="@font/rubik_regular"
                android:gravity="start"
                android:maxLines="1"
                android:paddingRight="@dimen/_10sdp"
                android:singleLine="true"
                android:text="@string/na_txt"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textSmallSize"
                android:visibility="gone"/>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.3dp"
            android:layout_gravity="end"
            android:layout_marginTop="@dimen/_10sdp"
            android:background="@color/new_add_item_line_color" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/_10sdp"
            android:fontFamily="@font/rubik_regular"
            android:gravity="start"
            android:text="@string/item_details"
            android:textColor="@color/new_sign_in_text_color"
            android:textSize="@dimen/textNormalSize" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5sdp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:fontFamily="@font/rubik_regular"
                    android:paddingRight="@dimen/_10sdp"
                    android:singleLine="true"
                    android:text="@string/item_title_heading"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/textSmallSize" />

                <TextView
