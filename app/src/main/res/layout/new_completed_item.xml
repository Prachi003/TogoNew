                            android:src="@drawable/new_app_icon1" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="vertical">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_2sdp">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:paddingLeft="@dimen/_4sdp"
                                    android:weightSum="1">

                                    <LinearLayout
                                        android:layout_width="0dip"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:orientation="vertical">

                                        <TextView
                                            android:id="@+id/title"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:ellipsize="end"
                                            android:fontFamily="@font/montserrat_semi_bold"
                                            android:maxLines="1"
                                            android:text="Dell Chromebook"
                                            android:textColor="@color/new_app_color"
                                            android:textSize="@dimen/textNormalSize" />

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="@dimen/_1sdp">

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:fontFamily="@font/montserrat_light"
                                                android:text="Pickup Date: "
                                                android:textColor="@color/new_gray_color"
                                                android:textSize="@dimen/item_size" />

                                            <TextView
                                                android:id="@+id/itemPickDt"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:fontFamily="@font/montserrat_medium"
                                                android:text="wqw"
                                                android:textColor="@color/new_sign_in_text_color"
                                                android:textSize="@dimen/item_size" />
                                        </LinearLayout>

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="@dimen/_2sdp">

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:fontFamily="@font/montserrat_light"
                                                android:text="Pickup Time: "
                                                android:textColor="@color/new_gray_color"
                                                android:textSize="@dimen/item_size" />

                                            <TextView
                                                android:id="@+id/itemPickTime"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:fontFamily="@font/montserrat_medium"
                                                android:text="wqw"
                                                android:textColor="@color/new_sign_in_text_color"
                                                android:textSize="@dimen/item_size" />
                                        </LinearLayout>

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="@dimen/_2sdp"
                                            android:gravity="center">

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:fontFamily="@font/montserrat_light"
                                                android:text="Address: "
                                                android:textColor="@color/new_gray_color"
                                                android:textSize="@dimen/item_size" />

                                            <TextView
                                                android:id="@+id/itemPickaddr"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginRight="@dimen/_5sdp"
                                                android:ellipsize="end"
                                                android:fontFamily="@font/montserrat_medium"
                                                android:singleLine="true"
                                                android:text="wewewee"
                                                android:textColor="@color/new_sign_in_text_color"
                                                android:textSize="@dimen/item_size" />
                                        </LinearLayout>
                                    </LinearLayout>

                                </LinearLayout>

                            </RelativeLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_marginTop="@dimen/_3sdp"
                                android:background="@color/new_post_item_border_color" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:padding="@dimen/_4sdp"
                                android:weightSum="2">

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_weight="1"
                                    android:gravity="center">

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="@font/montserrat_light"
                                        android:text="$ : "
                                        android:textColor="@color/new_gray_color"
                                        android:textSize="@dimen/item_size" />

                                    <TextView
                                        android:id="@+id/itemPrice"
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:fontFamily="@font/montserrat_medium"
                                        android:text="$ 2500563"
                                        android:textColor="@color/new_sign_in_text_color"
                                        android:textSize="@dimen/item_size" />
                                </LinearLayout>

                                <View
                                    android:layout_width="1dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="@dimen/_5sdp"
                                    android:background="@color/new_post_item_border_color" />

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_marginLeft="@dimen/_5sdp"
                                    android:layout_weight="1"
                                    android:gravity="center">

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="@font/montserrat_light"
                                        android:text="Quantity : "
                                        android:textColor="@color/new_gray_color"
                                        android:textSize="@dimen/item_size" />

                                    <TextView
                                        android:id="@+id/itemQuntyTxt"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="@font/montserrat_medium"
                                        android:text="dfgfgdgdf"
                                        android:textColor="@color/new_sign_in_text_color"
                                        android:textSize="@dimen/item_size" />
                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                    <RelativeLayout
