<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/llParentn"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/_5sdp"
    app:clickToClose="true">



    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:background="@drawable/new_change_password_items_bg"

        android:layout_height="wrap_content">
        <LinearLayout
            android:id="@+id/main_click"
            android:layout_width="match_parent"

            android:layout_height="@dimen/_90sdp"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_width="@dimen/_100sdp"
                android:background="@drawable/pending_placeholder"
                android:layout_height="match_parent">
                <LinearLayout
                    android:layout_width="100dp"
                    android:gravity="center"
                    android:layout_gravity="center"
                    android:background="@drawable/new_change_color_blue"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/etPricen"
                        android:layout_width="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/proxima_nova_semibold"
                        android:maxLines="1"
                        android:text="$ 125.0"
                        android:textColor="@color/colorWhite"
                        android:textSize="@dimen/textNormalSize" />

                    <TextView
                        android:id="@+id/tvtitle"
                        android:layout_width="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/montserrat_regular"
                        android:maxLines="2"
                        android:text="@string/total_delivery_price_n"
                        android:textColor="@color/colorWhite"
                        android:textSize="11sp" />


                </LinearLayout>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="wrap_content"
                    android:text="Laptop orders"
                    android:textSize="16sp"
                    android:id="@+id/txtTitlen"
                    android:textColor="@color/colorPrimary"
                    android:fontFamily="@font/montserrat_medium"
                    android:layout_margin="@dimen/_7sdp"
                    android:layout_height="wrap_content" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:layout_below="@+id/txtTitlen"

                    android:id="@+id/llOrders"
                    android:layout_marginBottom="@dimen/_10sdp"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:layout_alignParentBottom="true"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/_5sdp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:text="05"
                        android:textSize="14sp"
                        android:id="@+id/txtLaptopOrdern"
                        android:textColor="@color/colorBlack"
                        android:fontFamily="@font/rubik_medium"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:text="Delivery items"
                        android:textSize="12sp"
                        android:id="@+id/txtDelivery"
                        android:layout_below="@+id/txtLaptopOrdern"
                        android:layout_marginBottom="@dimen/_5sdp"
                        android:textColor="@color/darke_gray"
                        android:fontFamily="@font/montserrat_regular"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:text="1 day ago"
                        android:textSize="12sp"
                        android:id="@+id/txtTimeShown"
                        android:layout_marginRight="4sp"
                        android:layout_below="@+id/txtLaptopOrdern"

                        android:layout_centerVertical="true"
                        android:layout_alignParentEnd="true"
                        android:fontFamily="@font/montserrat_regular"

                        android:layout_height="match_parent"
                        android:layout_alignParentRight="true" />


                </RelativeLayout>


            </RelativeLayout>






            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:background="@drawable/new_change_password_items_bg">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
