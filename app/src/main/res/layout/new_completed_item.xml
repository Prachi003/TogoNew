                                android:textColor="@color/new_gray_color"
                                android:textSize="@dimen/item_size" />

                            <TextView
                                android:id="@+id/itemAsignTo"
                                android:layout_width="@dimen/_130sdp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="end"
                                android:gravity="end"
                                android:ellipsize="end"
                                android:singleLine="true"
                                android:fontFamily="@font/montserrat_medium"
                                android:textColor="@color/new_app_color"
                                android:textSize="@dimen/item_size" />
                        </LinearLayout>

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/iv_assign_img"
                            android:layout_width="@dimen/_30sdp"
                            android:layout_height="@dimen/_30sdp"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/_5sdp"
                            android:src="@drawable/new_app_icon" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/rl_courier_post_status"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10sdp"
                        android:layout_marginRight="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <TextView
                            android:id="@+id/tv_courier_post_status"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:fontFamily="@font/montserrat_regular"
                            android:text="@string/completed"
                            android:textColor="@color/colorBtn" />
                    </RelativeLayout>

                    <LinearLayout
                        android:id="@+id/contactLay"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_3sdp"
                        android:visibility="gone">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/proxima_nova_regular"
                            android:text="Contact to: "
                            android:textColor="@color/colorTextHint"
                            android:textSize="@dimen/item_size" />

                        <TextView
                            android:id="@+id/contact_num"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/proxima_nova_bold"
                            android:text=""
                            android:textColor="@color/colorTextHint"
                            android:textSize="@dimen/item_size" />
                    </LinearLayout>
                </LinearLayout>

